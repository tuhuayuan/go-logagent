{
    "input": [
        {
            "type": "stdin",
            "prefix": "[logagent#] "
        },
        {
            "type": "http",
            "host": "0.0.0.0:3000",
			"url_path": "/replay",
			"methods": ["POST","PUT"] 
        }
    ],
    "filter": [ 
        {
            "type": "patch",
            "key": "from",
            "value": "test_command"   
        },
        {
            "type": "grok",
            "match": "(?P<input_command>.*)"
        }
    ],   
    "output": [     
        {
            "type": "stdout"
        },
        {
            "type": "redis",
	    	"key": "log",
			"db": 1,
	        "host": "127.0.0.1:6379",
	        "data_type": "list",
	        "timeout": 5
        }
    ]
}