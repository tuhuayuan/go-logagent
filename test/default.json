{
    "input": [
        {
            "type": "stdin",
            "prefix": "[logagent#] "
        }
    ],
    "filter": [
        {
            "type": "patch",
            "key": "from",
            "value": "stdin"
        },
        {
            "type": "grok",
            "match": "(?P<input>.*)"
        }
    ],
    "output": [
        {
            "type": "stdout"
        }
    ]
}